{{!< layout}}
 <header class="aui-page-header">
     <div class="aui-page-header-inner">
         <div class="aui-page-header-main intro-header">
             <h1>{{title}}</h1>
         </div>
     </div>
 </header>

 <div class="aui-page-panel main-panel">
        <div class="aui-page-panel-inner">
            <table class="aui">
                <thead>
                    <tr>
                        <th>Proyecto</th>
                        <th>Acciones</th>
                    </tr>
                </thead>
                <tbody>
                    {{#each project}}
                    <tr>
                        <td headers="name">
                            <a href="https://urjctfg.atlassian.net/browse/" + {{this.key}} target="_top">
                                {{this.name}}</a>
                        </td>
                        <td headers="actions">
                            <button class="aui-button aui-button-primary dial-action">AÃ±adir tarea</button>
                        </td>
                    </tr>
                    {{/each}}
                </tbody>
            </table>
            <div id="dial-content" class="dialog-content" aria-hidden="true">
                <p>Bien!</p>
                <button id="dial-close" class="aui-button">Cerrar</button>
            </div>
        </div>
    </div>